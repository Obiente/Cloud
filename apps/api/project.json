{
  "name": "@obiente/api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/api",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@obiente-lab/nx-go:build",
      "options": {
        "main": "{projectRoot}/main.go",
        "outputPath": "{projectRoot}/api"
      }
    },
    "serve": {
      "executor": "@obiente/nxsh:run-sh",
      "options": {
        "script": "apps/api/scripts/serve-dev.sh",
        "cwd": "apps/api",
        "shell": true
      },
      "cache": false
    },
    "test": {
      "executor": "@obiente-lab/nx-go:test"
    },
    "lint": {
      "executor": "@obiente-lab/nx-go:lint"
    },
    "tidy": {
      "executor": "@obiente-lab/nx-go:tidy"
    }
  }
}
